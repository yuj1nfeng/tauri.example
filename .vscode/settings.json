{
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "typescript.tsdk": "node_modules/typescript/lib",
    "files.exclude": {
        "**/.git": true,
        "**/node_modules": true,
        "**/dist": false
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/dist": true
    },
    "typescript.preferences.importModuleSpecifier": "non-relative"
}
